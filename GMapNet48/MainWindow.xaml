<Window x:Class="GMapNet48.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GMapNet48"
        xmlns:gmap="clr-namespace:GMap.NET.WindowsPresentation;assembly=GMap.NET.WindowsPresentation"
        mc:Ignorable="d"
        Title="MainWindow" Height="1000" Width="1000" WindowState="Maximized">
    <Grid>
        <gmap:GMapControl x:Name="gMapControl" HorizontalAlignment="Left" Height="800" Width="800" VerticalAlignment="Top"/>
        <Button x:Name="MapButton" Content="클릭하세요" HorizontalAlignment="Left" VerticalAlignment="Bottom" Width="100" Height="50" Margin="0,0,0,300" Click="MapButton_Click"/>
    </Grid>
</Window>
